{
  "name": "fhirbuilder",
  "version": "1.0.2",
  "description": "Another FHIR Tool to build FHIR Resources",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "author": "Roberto Araneda",
  "repository": {
    "directory": "lib",
    "type": "git",
    "url": "git+https://github.com/robertoAraneda/fhir-builder"
  },
  "bugs": {
    "url": "https://github.com/robertoAraneda/fhir-builder/issues",
    "email": "robaraneda@gmail.com"
  },
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint && npm run test:unit",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "test": "jest --runInBand"
  },
  "keywords": [
    "FHIR",
    "Types",
    "Validator",
    "Resource"
  ],
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "ts-node-dev": "^2.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.4.5",
    "fhirtypes": "^1.0.25"
  },
  "files": [
    "lib/**/*",
    "README.md",
    "package.json",
    "LICENSE"
  ]
}
